// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Course{
  id_course        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  kode_course     String
  name_course   String
  jumlah_course   String
  desc_course  String
  image_course String
  vidio_course  String
}

model Categories {
  id_categories        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  kode_categories     String
  name_categories   String
  harga_categories  Int
  desc_categories   String
  image_categories  String
  jumlah_categories String
}

model registration {
  id_registration Int      @id @default(autoincrement())
  name String
  email String
  number Int
  course String
  gender String
}